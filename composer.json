    "homepage": "https://github.com/i-lateral/silverstripe-admin-toggle-checkbox",
    "keywords": [
        "silverstripe",
        "admin",
        "checkbox"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Morven Lewis-Everley",
            "email": "morven@ilateral.co.uk"
        },
        {
            "name": "Mark Anderson",
            "email": "mark@ilateral.co.uk"
        }
